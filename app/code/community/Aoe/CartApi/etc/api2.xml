<?xml version="1.0"?>
<config>
    <api2>
        <resource_groups>
            <aoe_cartapi translate="title" module="Aoe_CartApi">
                <title>AOE CartApi</title>
                <sort_order>100</sort_order>
            </aoe_cartapi>
        </resource_groups>
        <resources>
            <aoe_cartapi_cart translate="title" module="Aoe_CartApi">
                <versions>1</versions>
                <group>aoe_cartapi</group>
                <model>Aoe_CartApi/Cart</model>
                <title>Cart</title>
                <sort_order>10</sort_order>
                <privileges>
                    <guest>
                        <create>1</create>
                        <retrieve>1</retrieve>
                        <update>1</update>
                        <delete>1</delete>
                    </guest>
                    <customer>
                        <create>1</create>
                        <retrieve>1</retrieve>
                        <update>1</update>
                        <delete>1</delete>
                    </customer>
                </privileges>
                <attributes translate="" module="Aoe_CartApi">
                    <coupon_code>Coupon Code</coupon_code>
                    <!-- View-only -->
                    <items>Items</items>
                    <billing_address>Billing Address</billing_address>
                    <shipping_address>Shipping Address</shipping_address>
                    <payment>Payment</payment>
                    <shipping_method>Shipping Method</shipping_method>
                    <qty>Summary Quantity</qty>
                    <totals>Totals</totals>
                    <messages>Messages</messages>
                    <has_error>Has Error</has_error>
                </attributes>
                <exclude_attributes>
                    <guest>
                        <write>
                            <items>1</items>
                            <billing_address>1</billing_address>
                            <shipping_address>1</shipping_address>
                            <payment>1</payment>
                            <shipping_method>1</shipping_method>
                            <qty>1</qty>
                            <totals>1</totals>
                            <messages>1</messages>
                            <has_error>1</has_error>
                        </write>
                    </guest>
                    <customer>
                        <write>
                            <items>1</items>
                            <billing_address>1</billing_address>
                            <shipping_address>1</shipping_address>
                            <payment>1</payment>
                            <shipping_method>1</shipping_method>
                            <qty>1</qty>
                            <totals>1</totals>
                            <messages>1</messages>
                            <has_error>1</has_error>
                        </write>
                    </customer>
                </exclude_attributes>
                <routes>
                    <route_entity>
                        <route>/cart</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
            </aoe_cartapi_cart>
            <aoe_cartapi_item translate="title" module="Aoe_CartApi">
                <versions>1</versions>
                <group>aoe_cartapi</group>
                <model>Aoe_CartApi/Item</model>
                <title>Item</title>
                <sort_order>20</sort_order>
                <privileges>
                    <guest>
                        <create>1</create>
                        <retrieve>1</retrieve>
                        <update>1</update>
                        <delete>1</delete>
                    </guest>
                    <customer>
                        <create>1</create>
                        <retrieve>1</retrieve>
                        <update>1</update>
                        <delete>1</delete>
                    </customer>
                </privileges>
                <attributes translate="" module="Aoe_CartApi">
                    <item_id>Item ID (internal)</item_id>
                    <sku>SKU</sku>
                    <name>Name</name>
                    <images>Images</images>
                    <children>Child Items</children>
                    <qty>Quantity</qty>
                    <backorder_qty>Backorder Quantity</backorder_qty>
                    <original_price>Original Price</original_price>
                    <price>Price</price>
                    <row_total>Row Total</row_total>
                    <messages>Messages</messages>
                    <error_info>Error Info</error_info>
                    <is_saleable>Is Saleable</is_saleable>
                </attributes>
                <exclude_attributes>
                    <customer>
                        <write>
                            <item_id>1</item_id>
                            <name>1</name>
                            <images>1</images>
                            <children>1</children>
                            <backorder_qty>1</backorder_qty>
                            <original_price>1</original_price>
                            <price>1</price>
                            <row_total>1</row_total>
                            <messages>1</messages>
                            <error_info>1</error_info>
                            <is_saleable>1</is_saleable>
                        </write>
                    </customer>
                    <guest>
                        <write>
                            <item_id>1</item_id>
                            <name>1</name>
                            <images>1</images>
                            <children>1</children>
                            <backorder_qty>1</backorder_qty>
                            <original_price>1</original_price>
                            <price>1</price>
                            <row_total>1</row_total>
                            <messages>1</messages>
                            <error_info>1</error_info>
                            <is_saleable>1</is_saleable>
                        </write>
                    </guest>
                </exclude_attributes>
                <routes>
                    <route_collection>
                        <route>/cart/items</route>
                        <action_type>collection</action_type>
                    </route_collection>
                    <route_entity>
                        <route>/cart/items/:id</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
            </aoe_cartapi_item>
            <aoe_cartapi_billing_address translate="title" module="Aoe_CartApi">
                <versions>1</versions>
                <group>aoe_cartapi</group>
                <model>Aoe_CartApi/BillingAddress</model>
